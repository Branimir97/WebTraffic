{% extends 'base.html.twig' %}

{% block title %}WebTraffic{% endblock %}
{% block javascripts %}
    <script>
        $(document).ready(function() {
            var startTime = new Date();  
            $(window).on("unload", function() { 
                var endTime = new Date();  
                document.cookie="elapsed="+(endTime-startTime)/1000;
            });
        });
    </script>
{% endblock %}
{% block body %}
{{
    elapsed
}}
{% endblock %}

